filetype plugin indent on
set nocompatible
set esckeys
set backspace=indent,eol,start
set number
set tabstop=2
set shiftwidth=2
set expandtab
set list
set title
set showmode
set ruler
set lcs=tab:→\ ,trail:·,nbsp:_
set rtp+=~/.vim/bundle/Vundle.vim

call vundle#begin()
Plugin 'VundleVim/Vundle.vim'
Plugin 'preservim/nerdtree'
Plugin 'fatih/vim-go'
Plugin 'zxqfl/tabnine-vim'
call vundle#end()

syntax on
